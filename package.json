{
  "name": "clever-demo",
  "version": "1.0.0",
  "description": "This app for ES requirements",
  "main": "index.js",
  "scripts": {
    "serve": "serve dist",
    "start": "webpack-dev-server --env.mode development",
    "start:analyze": "webpack-dev-server --env.mode development --env.analyze",
    "build:dev": "rimraf dist && cross-env NODE_ENV=development webpack --env.mode development",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production webpack --env.mode production",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx}",
    "test": "jest",
    "test:watch": "jest --coverage --watch",
    "test:all": "set CI=true && yarn test && yarn lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix",
      "pre-push": "yarn test:all"
    }
  },
